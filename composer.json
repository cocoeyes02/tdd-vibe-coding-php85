{
    "name": "cocoeyes02/tdd-vibe-coding-php85",
    "description": "Claude Codeを使ったVibe CodingでTDD本をPHP 8.5で写経するプロジェクト",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
